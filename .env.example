# Women Safety App - Environment Variables

# Firebase Configuration (for production backend)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id

# Police API Configuration
VITE_POLICE_API_URL=http://localhost:3001/api
VITE_POLICE_API_KEY=your_police_api_key

# SMS/Notification Service (Twilio for emergency contacts)
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token
VITE_TWILIO_FROM_NUMBER=+1234567890

# Push Notification Service
VITE_PUSH_SERVICE_KEY=your_push_service_key

# Location Service
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# App Configuration
VITE_APP_NAME=SafeGuard
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_AUDIO_DETECTION=true
VITE_ENABLE_SHAKE_DETECTION=true
VITE_ENABLE_KEYWORD_DETECTION=true
VITE_ENABLE_LOCATION_TRACKING=true

# Model Configuration
VITE_AUDIO_THRESHOLD=60
VITE_DETECTION_CONFIDENCE_THRESHOLD=0.7
VITE_MODEL_PATH=/models/sound_classifier.tflite
